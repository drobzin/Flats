        <TextBlock HorizontalAlignment="Left" Margin="300,440,0,0" TextWrapping="Wrap" Text="RegID" VerticalAlignment="Top" Name="regid" />
        <TextBox HorizontalAlignment="Left" Margin="300,460,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Name="regid_box" />
        <Button Content="Добавить" HorizontalAlignment="Left" Margin="190,485,0,0" VerticalAlignment="Top" Name="NewCity" Click="Add_Click" />
        

