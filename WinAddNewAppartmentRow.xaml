        <TextBox HorizontalAlignment="Left" Margin="300,160,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Name="private_box" />
        <TextBlock HorizontalAlignment="Left" Margin="300,200,0,0" TextWrapping="Wrap" Text="Phone" VerticalAlignment="Top" Name="phone" />
        <TextBox HorizontalAlignment="Left" Margin="300,220,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Name="phone_box" />
