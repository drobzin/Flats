        <TextBlock HorizontalAlignment="Left" Margin="20,440,0,0" TextWrapping="Wrap" Text="TypeToilet" VerticalAlignment="Top" Name="type_toilet" />
        <TextBox HorizontalAlignment="Left" Margin="20,460,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Name="type_toilet_box" />
