<Window x:Class="Flats.WinAddNewClientphoneRow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flats"
        mc:Ignorable="d"
        Title="WinAddNewClientphoneRow" Height="270" Width="270">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="20,20,0,0" TextWrapping="Wrap" Text="RegID" VerticalAlignment="Top" Name="regid"/>
        <TextBox HorizontalAlignment="Left" Margin="20,40,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Name="regid_box" />
        <TextBlock HorizontalAlignment="Left" Margin="20,80,0,0" TextWrapping="Wrap" Text="Phone" VerticalAlignment="Top" Name="phone" />
        <TextBox HorizontalAlignment="Left" Margin="20,100,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Name="phone_box" />
       
       

        <Button Content="Добавить" HorizontalAlignment="Left" Margin="20,140,0,0" VerticalAlignment="Top" Name="NewCity" Click="Add_Click" />

    </Grid>
</Window>
